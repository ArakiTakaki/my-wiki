{
  "name": "electron-for-react-typescript",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "_ START系": "_",
    "start": "concurrently --kill-others \"yarn start:electron\" \"yarn start:client\"",
    "start:electron": "electron ./dist/electron/electron/main.js",
    "start:client": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack-dev-server -t webpack.config.ts",
    "_ BUILD系": "_",
    "build:electron": "tsc ./src/electron/main.ts --outDir ./dist/electron",
    "build": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "concurrently": "^4.1.0",
    "fecha": "^3.0.2",
    "mobx": "^5.9.0",
    "mobx-devtools": "^0.9.20",
    "mobx-react": "^5.4.3",
    "mobx-react-devtools": "^6.0.3",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "reset-css": "^4.0.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/electron": "^1.6.10",
    "@types/fecha": "^2.3.1",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^24.0.6",
    "@types/loader-utils": "^1.1.3",
    "@types/node": "^11.9.5",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.2",
    "@types/webpack": "^4.4.24",
    "@types/webpack-dev-server": "^3.1.2",
    "autodll-webpack-plugin": "^0.4.2",
    "autoprefixer": "^9.4.8",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "electron": "^4.0.5",
    "electron-builder": "^20.38.5",
    "electron-json-storage": "^4.1.6",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "loader-utils": "^1.2.3",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-rational-order": "^0.0.4",
    "stylelint-scss": "^3.5.4",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.5",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.0",
    "webpack-merge": "^4.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "*.{css,scss,sass}": [
      "stylelint --fix",
      "git add"
    ]
  }
